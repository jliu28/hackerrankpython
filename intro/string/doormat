# Designer Door Mat

N, M = map(int,input().split()) 
for i in range(0,int((N-1)/2)): 
    print (('.|.'*i).rjust(int((M-3)/2),'-')+'.|.'+('.|.'*i).ljust(int((M-3)/2),'-'))
print ('WELCOME'.center(M,'-'))
for i in reversed(range(0,int((N-1)/2))): 
    print (('.|.'*i).rjust(int((M-3)/2),'-')+'.|.'+('.|.'*i).ljust(int((M-3)/2),'-'))
